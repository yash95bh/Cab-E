<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_view"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="?actionBarSize"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/backArrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_margin="@dimen/_5sdp"
                android:padding="@dimen/_2sdp"
                android:src="@drawable/icon_backarrow_black"
                app:tint="@color/black" />

            <com.eviort.cabedriver.NTCustomView.NTBoldTextView
                android:id="@+id/lblTitle"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="@string/history"
                android:textColor="@color/black"
                android:textSize="@dimen/_15sdp" />
        </LinearLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1">

            <LinearLayout
                android:id="@+id/parentLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/tripImg"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_150sdp"
                        android:src="@drawable/placeholder"
                        android:visibility="gone" />

                    <com.eviort.cabedriver.NTCustomView.NTTextView
                        android:id="@+id/tv_create_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tripImg"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:text="Mon,Apr 24"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_20sdp">

                    </com.eviort.cabedriver.NTCustomView.NTTextView>

                    <com.eviort.cabedriver.NTCustomView.NTTextView
                        android:id="@+id/booking_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tv_create_time"
                        android:layout_alignParentBottom="true"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_marginBottom="@dimen/_10sdp"
                        android:text="CRN 45456098"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_15sdp">

                    </com.eviort.cabedriver.NTCustomView.NTTextView>
                </RelativeLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="@dimen/_15sdp"
                    android:layout_marginTop="@dimen/_20sdp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center_vertical">

                        <ImageView
                            android:id="@+id/start_iv"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:src="@drawable/location_pickup" />

                        <RelativeLayout
                            android:id="@+id/start_rv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:visibility="visible">

                            <com.eviort.cabedriver.NTCustomView.NTTextView
                                android:id="@+id/tripSource"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toStartOf="@+id/tripstart_at"
                                android:paddingHorizontal="@dimen/_10sdp"
                                android:singleLine="true"
                                android:textColor="@color/text_color_black"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />

                            <com.eviort.cabedriver.NTCustomView.NTTextView
                                android:id="@+id/tripstart_at"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:maxLines="1"
                                android:textColor="@color/text_color_black"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:gravity="center_vertical">

                        <ImageView
                            android:id="@+id/end_iv"
                            android:layout_width="@dimen/_10sdp"
                            android:layout_height="@dimen/_10sdp"
                            android:src="@drawable/location_drop"
                            tools:ignore="ContentDescription,RtlHardcoded" />

                        <RelativeLayout
                            android:id="@+id/end_rv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:visibility="visible">

                            <com.eviort.cabedriver.NTCustomView.NTTextView
                                android:id="@+id/tripDestination"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_alignParentTop="true"
                                android:singleLine="true"
                                android:layout_alignParentBottom="true"
                                android:layout_toStartOf="@+id/tripent_at"
                                android:paddingHorizontal="@dimen/_10sdp"
                                android:textColor="@color/text_color_black"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />

                            <com.eviort.cabedriver.NTCustomView.NTTextView
                                android:id="@+id/tripent_at"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_marginTop="@dimen/_2sdp"
                                android:maxLines="1"
                                android:textColor="@color/text_color_black"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible" />

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/profile_detail_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="8dp"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <com.eviort.cabedriver.NTCustomView.NTBoldTextView
                                android:id="@+id/tripProviderName"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/text_color_black"
                                android:textSize="@dimen/_14sdp" />

                            <com.eviort.cabedriver.NTCustomView.NTBoldTextView
                                android:id="@+id/tripservicetype"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/text_color_black"
                                android:textSize="@dimen/_14sdp"
                                android:visibility="gone" />

                            <RatingBar
                                android:id="@+id/tripProviderRating"
                                style="?attr/ratingBarStyleSmall"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="5dp"
                                android:isIndicator="true"
                                android:numStars="5"
                                android:progressTint="#FFCA28" />
                        </LinearLayout>

                        <com.eviort.cabedriver.NTCustomView.NTCircularImageView
                            android:id="@+id/tripProviderImg"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="@dimen/_50sdp"
                            android:layout_gravity="center"
                            android:layout_marginRight="8dp"
                            android:src="@drawable/place_holder" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="#1a1a1a" />

                </LinearLayout>

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_5sdp"

                    android:stretchColumns="1">

                    <!--<TableRow>-->

                    <!--<com.booktaxi.driver.NTCustomView.NTTextView-->
                    <!--style="@style/NTHintTextAppearance"-->
                    <!--android:layout_width="wrap_content"-->
                    <!--android:layout_height="wrap_content"-->
                    <!--android:layout_span="2"-->
                    <!--android:gravity="center"-->
                    <!--android:text="Invoice Details" />-->
                    <!--</TableRow>-->
                    <com.eviort.cabedriver.NTCustomView.NTTextView

                        style="@style/NTHintTextAppearance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/bill" />

                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/ridefare" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            android:id="@+id/tv_ridefare"
                            style="@style/NTLargeTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="$ 0" />
                    </TableRow>

                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/acce" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            android:id="@+id/tv_totalAccessFee"
                            style="@style/NTLargeTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="$ 0" />
                    </TableRow>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="@color/colorGreyMoreLighter" />


                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/coupon" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView
                            android:id="@+id/tv_coupon_savings"
                            style="@style/NTLargeTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="$ 0" />
                    </TableRow>

                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView
                            android:id="@+id/rounded_off"
                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tips" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            android:id="@+id/tv_totalFare"
                            style="@style/NTLargeTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="$ 0" />
                    </TableRow>


                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:layout_marginTop="@dimen/_2sdp"
                        android:background="@color/colorGreyMoreLighter" />


                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTBoldTextView

                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/total" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView
                            android:id="@+id/tv_total"
                            style="@style/NTRegularTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="$ 0" />
                    </TableRow>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="@color/colorGreyMoreLighter" />

                    <com.eviort.cabedriver.NTCustomView.NTBoldTextView

                        style="@style/NTHintTextAppearance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/payment_methods" />

                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView
                            android:id="@+id/pay_mode"
                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="start"
                            android:text="@string/invoice_cash" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            android:id="@+id/tv_total_cash"
                            style="@style/NTRegularTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="Booking Id"
                            android:visibility="gone" />
                    </TableRow>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.3dp"
                        android:background="@color/colorGreyMoreLighter" />

                    <com.eviort.cabedriver.NTCustomView.NTTextView
                        android:id="@+id/help"
                        style="@style/NTHintTextAppearance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_10"
                        android:paddingTop="@dimen/_10sdp"
                        android:paddingBottom="@dimen/_10sdp"
                        android:text="@string/gethelp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="@color/colorGreyMoreLighter" />

                    <com.eviort.cabedriver.NTCustomView.NTTextView
                        android:id="@+id/tv_get_invoice"
                        style="@style/NTHintTextAppearance"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableRight="@drawable/ic_baseline_arrow_forward_ios_10"
                        android:paddingTop="@dimen/_10sdp"
                        android:paddingBottom="@dimen/_10sdp"
                        android:text="@string/invoicecopy" />

                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView
                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/status"
                            android:visibility="gone" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            android:id="@+id/tv_status"
                            style="@style/NTRegularTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="Booking Id"
                            android:visibility="gone" />
                    </TableRow>

                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView
                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/distance"
                            android:visibility="gone" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            android:id="@+id/tv_distance"
                            style="@style/NTRegularTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="Booking Id"
                            android:visibility="gone" />
                    </TableRow>

                    <TableRow>

                        <com.eviort.cabedriver.NTCustomView.NTTextView

                            style="@style/NTHintTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/service_name"
                            android:visibility="gone" />

                        <com.eviort.cabedriver.NTCustomView.NTTextView
                            android:id="@+id/tv_service_name"
                            style="@style/NTRegularTextAppearance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center|right"
                            android:text="Booking Id"
                            android:visibility="gone" />
                    </TableRow>


                </TableLayout>

            </LinearLayout>
        </ScrollView>


    </LinearLayout>


</FrameLayout>



